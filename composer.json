{
    "name": "navjobs/temporal-models",
    "description": "Easily create Temporal Models with Laravel.",
    "keywords": [
        "laravel",
        "temporal",
        "trait"
    ],
    "homepage": "https://navjobs.com",
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/database": "^5.2",
        "illuminate/events": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0",
        "php-coveralls/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "NavJobs\\Temporal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NavJobs\\Temporal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
